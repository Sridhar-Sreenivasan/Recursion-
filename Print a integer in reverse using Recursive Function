include <iostream>
using namespace std;
void print_rev(int i) {
    if (i > 0) {
        cout << (i % 10);   // print last digit
        print_rev(i / 10);  // recursive call
    }
}
int main() {
    int num = 1122334;
    cout << "Reverse of " << num << " is: ";
    print_rev(num);
    return 0;
}
/*
Output :
Reverse of 1122334 is: 4332211
*/
